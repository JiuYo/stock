define(['jquery','common',"dao/unlineimpl/sqlitetools","model/UserModel"], function ($,common,sqlitetools) {　　　　
  var loginDao = {};
  
  loginDao.login = function (usermode,successcallback,errorcallback) {
  	alert("离线处理");
  	
  	//测试插入数据
  	var sql = 'insert into tb_d_user values(?,?,?)';
  	sqlitetools.executeSql(sql,[usermode.userName,usermode.userPwd,""],function(tx, rs){
  		alert("插入成功");
  	},function(tx,err){
  		alert("插入数据失败!"+err.message);
  	})
  	
  	
  	//测试获取数据
  	sql = "select * from tb_d_user";
  	sqlitetools.executeSql(sql,[],function(tx, rs){
  		alert("数据获取成功,共获取"+rs.rows.length+"条数据,数据内容如下:");
  		 for(var i = 0; i < rs.rows.length; i++){
            var row = rs.rows[i];
            alert(row.username+":"+row.userpassword);
        }
  	},function(tx,err){
  		alert("获取数据失败!"+err.message);
  	})
  	
  	
  	//测试删除数据
  	sql = "delete from tb_d_user";
  	sqlitetools.executeSql(sql,[],function(){alert("删除成功");});
  	
  	successcallback();
  };
  
  return loginDao;
});
